            .services__item-content
              h3.services__item-title.services__item-title--to-hide Нефтехимическая промышленность
              .services__item-num
                img(src="assets/img/svg/04.svg", width="270", height="220", alt="")
              .services__item-descrip.services__item-descrip--wide
                span.services__item-descrip-num 04.
                h3.services__item-title Нефтехимическая промышленность
                .services__item-descrip-text.text
